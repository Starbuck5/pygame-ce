SDL = -Iprebuilt-x64/SDL3-3.0.0/include -Iprebuilt-x64/SDL3-3.0.0/include/SDL3 -Lprebuilt-x64/SDL3-3.0.0/lib/x64  -lSDL3
FONT = -Iprebuilt-x64/SDL2_ttf-2.20.2/include -Lprebuilt-x64/SDL2_ttf-2.20.1/lib/x64  -lSDL2_ttf
IMAGE = -Iprebuilt-x64/SDL2_image-2.0.5/include -Lprebuilt-x64/SDL2_image-2.0.5/lib/x64  -lSDL2_image
MIXER = -Iprebuilt-x64/SDL2_mixer-2.6.3/include -Lprebuilt-x64/SDL2_mixer-2.6.2/lib/x64  -lSDL2_mixer
PORTMIDI = -Iprebuilt-x64/include -Lprebuilt-x64/lib  -lportmidi
PORTTIME = 
FREETYPE = -Iprebuilt-x64/include -Lprebuilt-x64/lib  -lfreetype
PNG = -Iprebuilt-x64/include -Lprebuilt-x64/SDL2_image-2.0.5/lib/x64  -llibpng16-16
JPEG = -Iprebuilt-x64/include -Lprebuilt-x64/SDL2_image-2.0.5/lib/x64  -llibjpeg-9
SCRAP = -luser32 -lgdi32
COPYLIB_SDL2 -Lprebuilt-x64/SDL3-3.0.0/lib/x64/SDL3.dll
COPYLIB_SDL2_ttf -lSDL -Lprebuilt-x64/SDL2_ttf-2.20.2/lib/x64/SDL2_ttf.dll
COPYLIB_SDL2_image -lSDL -ljpeg -lpng -ltiff -Lprebuilt-x64/SDL2_image-2.0.5/lib/x64/SDL2_image.dll
COPYLIB_SDL2_mixer -lSDL -Lprebuilt-x64/SDL2_mixer-2.6.3/lib/x64/SDL2_mixer.dll
COPYLIB_portmidi -Lprebuilt-x64/lib/portmidi.dll
COPYLIB_freetype -Lprebuilt-x64/lib/freetype.dll
COPYLIB_png -lz -Lprebuilt-x64/SDL2_image-2.0.5/lib/x64/libpng16-16.dll
COPYLIB_jpeg -Lprebuilt-x64/SDL2_image-2.0.5/lib/x64/libjpeg-9.dll
COPYLIB_ogg -Lprebuilt-x64/SDL2_mixer-2.6.3/lib/x64/optional/libogg-0.dll
COPYLIB_modplug -Lprebuilt-x64/SDL2_mixer-2.6.3/lib/x64/optional/libmodplug-1.dll
COPYLIB_opus -Lprebuilt-x64/SDL2_mixer-2.6.3/lib/x64/optional/libopus-0.dll
COPYLIB_opusfile -Lprebuilt-x64/SDL2_mixer-2.6.3/lib/x64/optional/libopusfile-0.dll
COPYLIB_tiff -ljpeg -lz -Lprebuilt-x64/SDL2_image-2.0.5/lib/x64/libtiff-5.dll
COPYLIB_z -Lprebuilt-x64/SDL2_image-2.0.5/lib/x64/zlib1.dll
COPYLIB_webp -Lprebuilt-x64/SDL2_image-2.0.5/lib/x64/libwebp-7.dll

